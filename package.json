{
	"name": "mr-blu-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "wrangler pages dev .svelte-kit/cloudflare",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"deploy": "npm run build && wrangler pages deploy .svelte-kit/cloudflare",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write .",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:unit": "vitest run",
		"test:watch": "vitest --watch",
		"test:coverage": "vitest run --coverage",
		"build:mobile": "BUILD_TARGET=capacitor vite build",
		"cap:ios": "npm run build:mobile && npx cap copy ios && npx cap open ios",
		"cap:android": "npm run build:mobile && npx cap copy android && npx cap open android"
	},
	"devDependencies": {
		"@cloudflare/puppeteer": "^1.0.5",
		"@eslint/js": "^9.39.2",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.1",
		"@vitest/coverage-v8": "^4.0.18",
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.0.0",
		"jsdom": "^27.4.0",
		"prettier": "^3.8.0",
		"prettier-plugin-svelte": "^3.4.1",
		"sharp": "^0.34.5",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^7.2.6",
		"vitest": "^4.0.18",
		"wrangler": "^4.54.0"
	},
	"dependencies": {
		"@capacitor/app": "^8.0.0",
		"@capacitor/cli": "^8.0.2",
		"@capacitor/core": "^8.0.2",
		"@capacitor/ios": "^8.0.2",
		"@supabase/ssr": "^0.8.0",
		"@supabase/supabase-js": "^2.89.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"gsap": "^3.14.2",
		"html2canvas": "^1.4.1",
		"jspdf": "^4.0.0",
		"jspdf-autotable": "^5.0.7",
		"lucide-svelte": "^0.562.0",
		"openai": "^6.15.0",
		"resend": "^6.7.0",
		"zod": "^4.3.5"
	}
}
